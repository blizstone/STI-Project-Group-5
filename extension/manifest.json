{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "contextMenus"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
    

      "128": "images/icon.png"
    }
  },
  "icons": {

    "128": "images/icon.png"
  },
  "manifest_version": 2
}
